{
  "name": "express-nodejs-template-api",
  "description": "API gateway template",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "readmeFilename": "README.md",
  "author": "Vitor Viana",
  "scripts": {
    "postinstall": "link-module-alias",
    "_preinstall": "link-module-alias clean",
    "link": "link-module-alias",
    "fixlint": "eslint . --fix",
    "lint": "eslint .",
    "develop": "nodemon --exec \"node -r dotenv/config\" ./bin/www.js",
    "start": "node ./bin/www.js",
    "integration-test": "mocha --opts tests/integration.opts",
    "unit-test": "mocha --opts tests/unit.opts",
    "test": "npm run lint && npm run unit-test && npm run integration-test",
    "heroku-prebuild": "node ./scripts/herokuPreBuildSourceVersion"
  },
  "_moduleAliases": {
    "$app-middleware": "./src/app-middleware",
    "$core-services": "./src/core-services",
    "$routes": "./src/routes/",
    "$config": "./src/config.js",
    "$services": "./src/services",
    "$database": "./src/database"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.4.2",
    "12factor-config": "^2.0.0",
    "api": "^3.3.0",
    "async-redis": "^1.1.7",
    "aws-sdk": "^2.843.0",
    "axios": "^0.21.0",
    "body-parser": "^1.13.3",
    "compression": "^1.6.2",
    "cookie-parser": "~1.3.5",
    "cors": "^2.8.4",
    "csv-parse": "^4.15.1",
    "csv-writer": "^1.6.0",
    "debug": "~2.6.7",
    "discord.js": "^12.5.3",
    "eth-sig-util": "^3.0.1",
    "ethereumjs-abi": "^0.6.8",
    "ethereumjs-util": "^7.0.7",
    "exceljs": "^4.2.0",
    "express": "~4.15.3",
    "fast-csv": "^4.3.6",
    "firstline": "^2.0.2",
    "git-rev": "^0.2.1",
    "link-module-alias": "^1.2.0",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "mongoose": "^5.11.11",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.16",
    "path": "^0.12.7",
    "pdf-creator-node": "^1.4.2",
    "pdf-image": "^2.0.0",
    "pdf-merger-js": "^3.0.5",
    "phantomjs": "^2.1.7",
    "qrcode": "^1.4.4",
    "read-excel-file": "^4.1.0",
    "readline": "^1.3.0",
    "remotelog-local": "^1.4.1",
    "swagger-jsdoc": "^1.9.7",
    "swagger-ui-express": "^2.0.15"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "dotenv": "^5.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^3.4.2",
    "nodemon": "^1.15.1",
    "proxyquire": "^2.0.1",
    "sinon": "^4.4.2"
  }
}
